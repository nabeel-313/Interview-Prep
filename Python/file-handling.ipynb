   "cell_type": "raw",
   "metadata": {
    "vscode": {
     "languageId": "raw"
    }
   },
   "source": [
    "with open('file.txt', 'r') as file:\n",
    "    content = file.read()       # Reading the entire file\n",
    "    first_line = file.readline() # Reading the first line\n",
    "    lines = file.readlines()    # Reading all lines into a list\n"
   ]
