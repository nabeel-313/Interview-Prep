   "source": [
    "#top performing product in earch year and quarter\n",
    "column are as\n",
    "Product_id INT,\n",
    "    Product_name VARCHAR(50),\n",
    "    sale_Date DATE,\n",
    "    sale_qty INT\n",
    "\n",
    "\n",
    "with CTE as (SELECT Product_id, Product_name,sale_Date,sale_qty,\n",
    "\tYEAR(sale_Date) AS sale_year,\n",
    "\tQUARTER(sale_Date) AS sale_quarter,\n",
    "    CONCAT(YEAR(sale_Date), '-Q', QUARTER(sale_Date)) AS sale_period,\n",
    "    ROW_NUMBER() OVER (partition by YEAR(sale_Date), QUARTER(sale_Date) order by sale_qty DESC) as rnk\n",
    "FROM prd_sales)\n",
    "select Product_id, Product_name, sale_period, sale_qty\n",
    "from CTE where rnk=1;\n",
    "\n"
   ]
