    "pair child with adult\n",
    "with adult AS\n",
    "    (select *, row_number() OVER(ORDER by person) as rn\n",
    "    where type = 'adult'\n",
    "    )\n",
    "with child as\n",
    "    ((select *, row_number() OVER(ORDER by person) as rn\n",
    "    where type = 'child'\n",
    "    )\n",
    "select a.person , c.person from adult a\n",
    "LEFT JOIN child c  on a.rn=c.rn\n"
   ]
