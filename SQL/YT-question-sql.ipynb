   "source": [
    "#find the increasing rev every year\n",
    "2 table sale and products\n",
    "\n",
    "with sale_CTE as (\n",
    "select *, lead(total_sales_revenue,1) OVER(partition by product_id order by year) as nxtyearrev,\n",
    "lead(total_sales_revenue,2) OVER(partition by product_id order by year) as nxt2nxtyearrev\n",
    "from sales)\n",
    "select p.product_id, p.product_name, p.category from products p\n",
    "join sale_CTE s on s.product_id = p.product_id\n",
    "where s.total_sales_revenue < s.nxtyearrev\n",
    "and s.nxtyearrev < s.nxt2nxtyearrev\n"
   ]
  },
