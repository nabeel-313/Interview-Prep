    "\n",
    "with CTE as (select * , ROW_NUMBER() OVER(partition by student_id order by marks desc) as rnk from table\n",
    "select student_id, sum(marks) as total from CTE\n",
    "where rnk <= 2\n",
    "group by student_id\n",
    "having sum(marks) >= 160\n"
