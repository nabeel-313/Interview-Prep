    "#Q1. Find the running cumulative total demand it is running sum aslo\n",
    "\n",
    "\n",
    "\n",
    "SELECT day,\n",
    "       qty,\n",
    "       SUM(qty) OVER(ORDER BY day) as cumQty\n",
    "FROM demand2;\n",
    "\n",
    "if cumulative with product wise\n",
    "\n",
    "SELECT product,\n",
    "\tday,\n",
    "        qty,\n",
    "        SUM(qty) OVER(PARTITION BY product ORDER BY day) as CUMSUM\n",
    "FROM demand;\n"
   ]
  },
