    "top 5 product in each category\n",
    "select * from (\n",
    "    select category, product, sum(sales) as total_sale\n",
    "    rank() over (partition by product order by total_sale decs) as rnk\n",
    "from order\n",
    "group by product category ) A\n",
    "where rnk <= 5\n"
