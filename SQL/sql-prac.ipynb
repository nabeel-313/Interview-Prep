    "each day which product sold max\n",
    "\n",
    "\n",
    "with tbl as (\n",
    "  select day,\n",
    "          product,\n",
    "          qty,\n",
    "          max(qty) over (partition by day) as maxqty\n",
    "  from demand)\n",
    "select day,\n",
    "\tproduct\n",
    "from tbl\n",
    "where qty = maxqty;\n",
    "\n"
