    "select * from(\n",
    "    select * ,\n",
    "        lag(free,1) over (order by seat_id) as pre_free,\n",
    "        lead(free,1) over (order by seat_id) as next_free\n",
    "from table_name ) A\n",
    "where free = 1 AND (pre_free = 1 or next_free = 1)\n"
