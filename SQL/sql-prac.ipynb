    "highest avg rated movie in each gener\n",
    "with cte as (\n",
    "    select m.gener, m.title AVG(r.rating) as avg_rating\n",
    "    ROW_NUMBER() OVER(partition by m.gener order by AVG(r.rating)) as rnk\n",
    "    from movie m INNER JOIN rating r on m.id = r.movie_id\n",
    "    group by m.gener, m.title )\n",
    "select * from cte where rnk = 1\n"
   ]
