   "source": [
    "When are the top 2 worst performing days for each product\n",
    "\n",
    "with tbl as (SELECT PRODUCT,\n",
    "                    qty,\n",
    "                    day,\n",
    "                    row_number() over (partition by product order by qty) as RN\n",
    "FROM demand)\n",
    "select product, day, qty\n",
    "from tbl a\n",
    "where RN in (1,2)\n"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {
    "vscode": {
     "languageId": "raw"
    }
   },
   "source": [
    "Find the percentage increase in qty compared to the previous day.\n",
    "\n",
    "\n",
    "with tbl as (select product,\n",
    "                    day,\n",
    "                    qty,\n",
    "                    lag(qty, 1) over (partition by product order by day) as qty_lag\n",
    "from demand)\n",
    "select *,\n",
    "round(((qty - qty_lag)/qty_lag) * 100, 2) as perc_increase\n",
    "from tbl\n",
    "where qty_lag is not null;\n",
    "\n"
   ]
  },
